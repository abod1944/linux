# OnePlus 6
CONFIG_DRM_PANEL_SAMSUNG_SOFEF00=y
CONFIG_BATTERY_BQ27XXX=m
# Touchscreen
CONFIG_HID_RMI=m
CONFIG_RMI4_CORE=m
CONFIG_RMI4_I2C=m
CONFIG_RMI4_F55=y

CONFIG_INPUT_QCOM_SPMI_HAPTICS=y

CONFIG_DRM=y
CONFIG_DRM_FBDEV_EMULATION=y
CONFIG_DRM_MSM=y

# AOSP

CONFIG_SECURITY_NETWORK=y
CONFIG_SECURITY_SELINUX=y

# (maybe) Fix stuck on boot
CONFIG_KASAN=n
CONFIG_UBSAN=n

# Required by init
CONFIG_NFT_NAT=m

